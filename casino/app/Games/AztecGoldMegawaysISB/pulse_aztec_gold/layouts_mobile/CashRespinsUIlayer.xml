                    <Element name="freeSpinsTotalWinValue" y="70" height="65" depth="0" class="highlightValuePortrait"/>
                </Element>
            </Element>
        </Element>
    </Main_portrait>
    <Classes>
        <Class name="highlightLabelLandscape" type="textField" x="0" width="140" labelFontSize="18" align="center"
               vAlign="center" labelFontColor="0xcccccc" labelFont="noto_sanscondensed_bold"/>
        <Class name="highlightValueLandscape" type="textField" x="0" width="140" labelFontSize="24" align="center"
               vAlign="center" labelFontColor="0xffffff" labelFont="noto_sanscondensed_bold"/>
        <Class name="defaultLabelLandscape" type="textField" x="0" y="6" height="15" depth="1" width="168"
               labelFontSize="22" align="center" vAlign="center" labelFontColor="0xcccccc" labelFont="noto_sanscondensed_bold"/>
        <Class name="defaultValueLandscape" type="textField" x="0" y="27" height="22" depth="2" width="168"
               labelFontSize="29" align="center" vAlign="center" labelFontColor="0xffffff" labelFont="noto_sanscondensed_bold"/>
        <Class name="highlightLabelPortrait" type="textField" x="35" width="480" labelFontSize="48" align="center"
               vAlign="center" labelFontColor="0xcccccc" labelFont="noto_sanscondensed_bold"/>
        <Class name="highlightValuePortrait" type="textField" x="35" width="480" labelFontSize="60" align="center"
               vAlign="center" labelFontColor="0xffffff" labelFont="noto_sanscondensed_bold"/>
        <Class name="defaultLabelPortrait" type="textField" x="35" y="12" width="370" height="28" depth="1"
               labelFontSize="30" align="center" vAlign="center" labelFontColor="0xcccccc" labelFont="noto_sanscondensed_bold"/>
        <Class name="defaultValuePortrait" type="textField" x="35" y="45" width="370" height="40" depth="1"
               labelFontSize="40" align="center" vAlign="center" labelFontColor="0xffffff" labelFont="noto_sanscondensed_bold"/>
        <Class name="bgSmallPortrait" x="0" y="0" width="440" height="90" depth="0" type="image"
               resource="MUIPortraitFreeSpinsInfoPanel" labelFont="noto_sanscondensed_bold"/>
    </Classes>
</Layout>