        <FontStyle name="mnButtonLandscape" fontSize="40" fontColor="0x000000" align="center" vAlign="center"/>
        <FontStyle name="mnButtonPortrait" fontSize="70" fontColor="0x000000" align="center" vAlign="center"/>
        <FontStyle name="mnTitleLandscape" fontSize="25" fontColor="0xFFFFFF" align="center" vAlign="center"/>
        <FontStyle name="mnTitlePortrait" fontSize="35" fontColor="0xFFFFFF" align="center" vAlign="center"/>
        <FontStyle name="mnTextBodyLandscape" fontSize="40" fontColor="0xFFFFFF" align="center" vAlign="center"/>
        <FontStyle name="mnTextBodyPortrait" fontSize="50" fontColor="0xFFFFFF" align="center" vAlign="center"/>
        
    </Styles>
    <Classes>
        
        <Class name="menuButtonLandscape" depth="5" type="button" x="10" width="50" height="50" disabledState=""/>
        <Class name="menuButtonPortrait" depth="5" type="button" y="5" width="60" height="60" disabledState=""/>
        <Class name="menuContainerLandscape" depth="4" type="container" x="0" y="0" width="920" height="510"/>
        <Class name="menuContainerPortrait" depth="4" type="container" x="0" y="0" width="800" height="1140"/>
        
    </Classes>

</Layout>