                <Element name="coinValuePanelButton" x="540"
                         upState="MUIMenuCoinValueButtonUp" pressedState="MUIMenuCoinValueButtonDown" disabledState="MUIMenuCoinValueButtonDown"
                         overState="MUIMenuCoinValueButtonUp"
                         class="menuButtonPortrait"/>
                <Element name="menuBackButton" x="680"
                         upState="MUIMenuBackButtonUp" pressedState="MUIMenuBackButtonDown" disabledState="MUIMenuBackButtonDown"
                         overState="MUIMenuBackButtonUp"
                         class="menuButtonPortrait"/>
            </Element>
        </Element>
