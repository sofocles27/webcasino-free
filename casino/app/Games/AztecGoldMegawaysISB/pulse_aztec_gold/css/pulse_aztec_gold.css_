#game {
	position: absolute;
	top: 0;
	left: 0;
}

.maskDesktop {
	width: 1200px;
	height: 600px;
	background-color: black;
	opacity: 0.9;
	position: fixed;
	display: block;
}

.maskMobile {
	display: none;
}

#swipe_overlay {
	position: absolute;
	width: 100vw;
	height: 10000px;
	z-index: 8990;
	background-color: black;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

#swipe_container {
	position: absolute;
	width: 100vw;
	height: 100vh;
	min-height: 100vh;
	min-width: 100vw;
	z-index: 8991;
}

#arrow {
	width: 100%;
	height: 44%;
	z-index: 8992;
	pointer-events: none;
}

#arrow > .arrow_img {
	width: 100%;
	height: 100%;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: center top;
	background-size: contain;
	z-index: 8993;
	pointer-events: none;
}

#circle_static {
	width: 100%;
	height: 30%;
	pointer-events: none;
}

#circle_static > .swipe_img {
	width: 100%;
	height: 100%;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: center top;
	background-size: contain;
	z-index: 8994;
	pointer-events: none;
}

#circle_move {
	position: absolute;
	top: 44%;
	width: 100%;
	height: 30%;
	pointer-events: none;
}

canvas {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 9;
}

#circle_move > .swipe_img {
	width: 100%;
	height: 100%;
	z-index: 8992;
	pointer-events: none;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: center top;
	background-size: contain;
	-webkit-animation: swipe 2s linear 1s infinite;
	-moz-animation: swipe 2s linear 1s infinite;
	animation: swipe 2s linear 1s infinite;
}

#layer_container {
	z-index: 10;
	position: absolute;
	pointer-events: none;
	top: 0;
	left: 0;
}

.layer {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
}

