header { min-height: 100vh; }

.noselect {
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-touch-callout: none;
	-ms-touch-action: none;
	touch-action: pan-y;
}

.noDrag {
	user-drag: none;
	user-select: none;
	-moz-user-select: none;
	-webkit-user-drag: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	-khtml-user-select: none;
}

/* context menu */
.context-menu {
	display: none;
	position: absolute;
	z-index: 9999;
	width: 190px;
	color: #56f9fd;
	background-color: #000;
	border: solid 1px #51cad8;
}

.context-menu--active {
	display: block;
}

.context-menu__items {
	padding: 0;
	padding-left: 32px;
}

.context-menu__item {
	display: none;
}

input:focus {
	outline-width: 0;
}

select:focus {
	outline-width: 0;
}

span {
	pointer-events: none;
}

html, body {
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	overflow: hidden;
	background-color: rgb(19, 19, 19);
}

body {
	font-family: noto_sansregular;
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center;
}

